{
	"schemaVersion": 1,
	"id": "fabric-example-mod",
	"version": "${version}",
	"name": "Fabric Example Mod",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/fabric-example-mod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"karasu_lab.fabric_example.FabricExampleMod"
		],
		"client": [
			"karasu_lab.fabric_example.FabricExampleModClient"
		],
		"fabric-datagen": [
			"karasu_lab.fabric_example.FabricExampleModDataGenerator"
		]
	},
	"mixins": [
		"fabric-example-mod.mixins.json",
		{
			"config": "fabric-example-mod.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.7",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}